{
  "name": "@bemedev/basicfunc",
  "version": "0.0.1",
  "description": "Basic ts functions",
  "author": {
    "email": "bri_lvi@icloud.com",
    "name": "chlbri",
    "url": "https://bemedev.vercel.app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chlbri/basifunc"
  },
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.js",
  "maintainers": [
    {
      "email": "bri_lvi@icloud.com",
      "name": "chlbri",
      "url": "https://bemedev.vercel.app"
    }
  ],
  "contributors": [
    {
      "email": "bri_lvi@icloud.com",
      "name": "chlbri",
      "url": "https://bemedev.vercel.app"
    }
  ],
  "keywords": [
    "types",
    "usefull"
  ],
  "files": [
    "lib",
    "public"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "pnpm run rm:lib && pnpm run rollup",
    "ci": "pnpm run config:off && pnpm run lint && pnpm run test",
    "ci:online": "pnpm run config && pnpm run lint && pnpm run test",
    "config": "pnpm install",
    "config:off": "pnpm run config --offline",
    "dep:off": "pnpm run dep --offline",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "p-q": "pretty-quick --staged",
    "posttest": "tsx ./vitest.after.ts",
    "posttest:watch": "tsx ./vitest.after.ts",
    "prepare": "husky",
    "pretest": "pnpm run build && tsx ./vitest.before.ts",
    "pretest:watch": "pnpm run build && tsx ./vitest.before.ts",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "rinit": "pnpm run rm && pnpm run config",
    "rinit:off": "pnpm run rinit --offline",
    "rm": "rm -rf node_modules pnpm-lock.yaml",
    "rm:lib": "rm -rf lib",
    "rollup": "rollup -c",
    "test": "pnpm run test:base --no-coverage",
    "test:base": "vitest run --passWithNoTests",
    "test:coverage": "pnpm run test:base",
    "test:watch": "vitest --passWithNoTests",
    "upgrade": "pnpm upgrade --latest"
  },
  "devDependencies": {
    "@bemedev/fsf": "^0.7.3",
    "@bemedev/sleep": "^0.1.2",
    "@bemedev/vitest-alias": "^0.0.3",
    "@bemedev/vitest-exclude": "^0.0.2",
    "@bemedev/vitest-extended": "^0.5.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/edit-json-file": "^1.7.3",
    "@types/node": "^22.10.5",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vscode/test-electron": "^2.4.1",
    "edit-json-file": "^1.8.0",
    "eslint": "^9.17.0",
    "glob": "^11.0.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "onchange": "^7.1.0",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "rollup": "^4.30.1",
    "rollup-plugin-circular-dependencies": "^1.1.2",
    "rollup-plugin-node-externals": "^8.0.0",
    "rollup-plugin-tsc-alias": "^1.1.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "shelljs": "^0.8.5",
    "sort-keys": "^5.1.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "@bemedev/types": "^0.1.3"
  }
}
