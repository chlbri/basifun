{
  "name": "@bemedev/basicfunc",
  "version": "0.0.2",
  "description": "Basic ts functions",
  "author": {
    "email": "bri_lvi@icloud.com",
    "name": "chlbri",
    "url": "https://bemedev.vercel.app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chlbri/basifunc"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "node": "./lib/index.cjs",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./common/*": {
      "import": "./lib/functions/common/*.js",
      "node": "./lib/functions/common/*.cjs",
      "require": "./lib/functions/common/*.cjs",
      "types": "./lib/functions/common/*.d.ts",
      "default": "./lib/functions/common/*.js"
    },
    "./numbers/*": {
      "import": "./lib/functions/numbers/*.js",
      "node": "./lib/functions/numbers/*.cjs",
      "require": "./lib/functions/numbers/*.cjs",
      "types": "./lib/functions/numbers/*.d.ts",
      "default": "./lib/functions/numbers/*.js"
    },
    "./strings/*": {
      "import": "./lib/functions/strings/*.js",
      "node": "./lib/functions/strings/*.cjs",
      "require": "./lib/functions/strings/*.cjs",
      "types": "./lib/functions/strings/*.d.ts",
      "default": "./lib/functions/strings/*.js"
    },
    "./types/*": {
      "import": "./lib/functions/types/*.js",
      "node": "./lib/functions/types/*.cjs",
      "require": "./lib/functions/types/*.cjs",
      "types": "./lib/functions/types/*.d.ts",
      "default": "./lib/functions/types/*.js"
    },
    "./arrays/*": {
      "import": "./lib/functions/arrays/*.js",
      "node": "./lib/functions/arrays/*.cjs",
      "require": "./lib/functions/arrays/*.cjs",
      "types": "./lib/functions/arrays/*.d.ts",
      "default": "./lib/functions/arrays/*.js"
    },
    "./objects/*": {
      "import": "./lib/functions/objects/*.js",
      "node": "./lib/functions/objects/*.cjs",
      "require": "./lib/functions/objects/*.cjs",
      "types": "./lib/functions/objects/*.d.ts",
      "default": "./lib/functions/objects/*.js"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.js",
  "maintainers": [
    {
      "email": "bri_lvi@icloud.com",
      "name": "chlbri",
      "url": "https://bemedev.vercel.app"
    }
  ],
  "contributors": [
    {
      "email": "bri_lvi@icloud.com",
      "name": "chlbri",
      "url": "https://bemedev.vercel.app"
    }
  ],
  "keywords": [
    "types",
    "usefull"
  ],
  "files": [
    "lib",
    "public"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "pnpm run rm:lib && pnpm run rollup",
    "ci": "pnpm run config:off && pnpm run lint && pnpm run test && pnpm run prettier && pnpm run p-q",
    "config": "pnpm install",
    "config:off": "pnpm run config --offline",
    "dep:off": "pnpm run dep --offline",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "p-q": "pretty-quick",
    "posttest": "pnpm build-tests post:test && pnpm run p-q",
    "posttest:watch": "pnpm run posttest",
    "pretest": "pnpm run build && pnpm build-tests pre:test",
    "pretest:watch": "pnpm run pretest",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "rinit": "pnpm run rm && pnpm run config",
    "rinit:off": "pnpm run rinit --offline",
    "rm": "rm -rf node_modules pnpm-lock.yaml",
    "rm:lib": "rm -rf lib",
    "rollup": "rollup -c",
    "test": "pnpm run test:coverage --no-coverage",
    "test:only": "pnpm run test:coverage --no-coverage",
    "test:coverage": "vitest run",
    "test:watch": "vitest",
    "upgrade": "pnpm upgrade --latest"
  },
  "devDependencies": {
    "@bemedev/build-tests": "^0.1.0",
    "@bemedev/fsf": "^0.7.3",
    "@bemedev/sleep": "^0.1.2",
    "@bemedev/vitest-alias": "^0.0.3",
    "@bemedev/vitest-exclude": "^0.0.2",
    "@bemedev/vitest-extended": "^0.6.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/edit-json-file": "^1.7.3",
    "@types/node": "^22.10.7",
    "@types/semver": "^7.5.8",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitest/coverage-v8": "^3.0.0",
    "edit-json-file": "^1.8.0",
    "eslint": "^9.18.0",
    "glob": "^11.0.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "onchange": "^7.1.0",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "rollup": "^4.30.1",
    "rollup-plugin-circular-dependencies": "^1.1.2",
    "rollup-plugin-node-externals": "^8.0.0",
    "rollup-plugin-tsc-alias": "^1.1.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "semver": "^7.6.3",
    "shelljs": "^0.8.5",
    "sort-keys": "^5.1.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@bemedev/types": "^0.1.3"
  }
}
