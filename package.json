  "scripts": {
    "build": "pnpm run lib:rm && pnpm run rollup",
    "build:pack": "pnpm run rm && pnpm run config:off && pnpm run build && pnpm run pack",
    "ci": "pnpm run build:pack && pnpm run lint && pnpm run test && pnpm run build",
    "ci:online": "pnpm run config && pnpm run lint && pnpm run test && pnpm run build",
    "config": "pnpm install",
    "config:off": "pnpm run config --offline",
    "dep:off": "pnpm run dep --offline",
    "eslint": "eslint ",
    "lib:rm": "rm -rf lib",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "pack": "tsx ./.pack/toPack.ts",
    "p-q": "pretty-quick --staged",
    "prepare": "husky",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "rinit": "pnpm run rm && pnpm run config",
    "rinit:off": "pnpm run rinit --offline",
    "rm": "rm -rf node_modules pnpm-lock.yaml",
    "rollup": "rollup -c",
    "test": "pnpm run test:coverage --no-coverage",
    "test:coverage": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "upgrade": "pnpm upgrade --latest"
  },
